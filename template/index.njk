const files = require.context('./', false, /\.js$/)
const apis = files.keys().filter((key) => !key.includes('index.js')).map(key => files(key).default)
{% set dash = joiner("/") %}
const moduleName = '{% for item in dirName %}/{{item }}{% endfor %}'

export default ({
  request,
  tools
}) => apis.map(api => api({
  request,
  tools,
  moduleName
}))
